{
    "name": "arp-api",
    "version": "0.4.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm install && node --experimental-modules --loader ts-node/esm src/server.ts",
        "start:dev": "npm install && node --experimental-modules --loader ts-node/esm src/server.ts",
        "build": "npm install && tsc -p .",
        "postbuild": "npm run copyDist && npm run copyAssets",
        "copyDist": "shx cp -R src/dist dist",
        "copyAssets": "shx cp -R src/assets dist",
        "dev": "npm install && ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts"
    },
    "dependencies": {
        "@apidevtools/swagger-parser": "^10.0.2",
        "axios": "^0.22.0",
        "body-parser": "^1.19.0",
        "cielo": "^2.4.1",
        "cloudinary": "^1.26.3",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "fs": "0.0.1-security",
        "html-pdf": "^2.2.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mongoose": "^5.11.15",
        "mysql2": "^2.2.5",
        "nodemailer": "^6.5.0",
        "pdf-creator-node": "^2.2.2",
        "pug": "^3.0.2",
        "sequelize": "^6.5.0",
        "swagger-jsdoc": "6.0.0",
        "swagger-ui-express": "^4.1.6"
    },
    "devDependencies": {
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/html-pdf": "^2.2.0",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/nodemailer": "^6.4.1",
        "@types/request": "^2.48.7",
        "@types/swagger-jsdoc": "^6.0.0",
        "@types/swagger-ui-express": "^4.1.2",
        "nodemon": "^2.0.7",
        "shx": "^0.3.3",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.4"
    }
}
